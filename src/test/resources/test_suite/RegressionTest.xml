<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="intive - Regression">
    <listeners>
        <listener class-name="com.automation.qa.ttafuicore.util.LocalWebDriverListener"/>
    </listeners>
    <test name="Regression - All">
        <parameter name="browserName" value="firefox"/>
        <classes>
                <class name="runner.RegressionTest"/>
        </classes>
    </test>
</suite>